{
  "compilerOptions": {
    "target": "es2016",
    /* Modules */
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["src/swagger-output.json", "src/**/*"]
}
